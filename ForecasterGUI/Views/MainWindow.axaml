<rxui:ReactiveWindow x:TypeArguments="vm:MainWindowViewModel" xmlns="https://github.com/avaloniaui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vm="using:ForecasterGUI.ViewModels"
                     xmlns:rxui="http://reactiveui.net"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                     x:Class="ForecasterGUI.Views.MainWindow"
                     Icon="/Assets/avalonia-logo.ico"
                     Width="1120" Height="840"
                     Title="ForecasterGUI"
                     Content="{Binding Content}">
    <rxui:ReactiveWindow.Styles>
        <StyleInclude Source = "/Styles/Style.axaml" />
    </rxui:ReactiveWindow.Styles>
</rxui:ReactiveWindow>
